{
    "name": "jdolba/silverstripe-chapters",
    "description": "'Chapters' a SilverStripe module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "roave/security-advisories": "dev-master",
        "jdolba/silverstripe-coding-standards": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "JDolba\\Chapters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JDolba\\Chapters\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "scripts": {
        "silverstripe-standards": [
            "@php-lint",
            "@phpcs"
        ],
        "php-lint": "parallel-lint src tests",
        "phpcs": "phpcs src tests --standard=vendor/jdolba/silverstripe-coding-standards/definitions/php/phpcs-ss4.xml --extensions=php --encoding=utf-8",
        "phpcbf": "phpcbf src tests --standard=vendor/jdolba/silverstripe-coding-standards/definitions/php/phpcs-ss4.xml --extensions=php --encoding=utf-8",
        "phpcs-fix": "@phpcbf"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
